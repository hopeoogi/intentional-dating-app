
{
  "expo": {
    "name": "Intentional",
    "slug": "intentional-dating",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/96e0c1f0-fcef-4b76-b942-74280a3296cb.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/96e0c1f0-fcef-4b76-b942-74280a3296cb.png",
      "resizeMode": "contain",
      "backgroundColor": "#5B4FE9"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.intentional.dating",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to upload profile photos",
        "NSCameraUsageDescription": "We need access to your camera to take profile photos",
        "NSLocationWhenInUseUsageDescription": "We need your location to show you matches nearby"
      },
      "buildNumber": "1.0.2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/96e0c1f0-fcef-4b76-b942-74280a3296cb.png",
        "backgroundColor": "#5B4FE9"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.intentional.dating",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/images/96e0c1f0-fcef-4b76-b942-74280a3296cb.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Intentional to use your location to show you matches nearby."
        }
      ],
      "expo-image-picker",
      "expo-secure-store"
    ],
    "scheme": "intentional",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-project-id"
      },
      "backendUrl": "https://6ytjugugmhrw5w5dguny2u79uz83r3tz.app.specular.dev"
    }
  }
}
