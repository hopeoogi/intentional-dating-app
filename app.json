{
  "expo": {
    "name": "Intentional Dating",
    "slug": "intentional-dating",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/natively-dark.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/natively-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.intentionaldating.app",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "We need access to your camera to take profile photos and videos.",
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to upload profile photos and videos.",
        "NSMicrophoneUsageDescription": "We need access to your microphone to record profile videos."
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/natively-dark.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.intentionaldating.app",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Intentional Dating to access your photos to upload profile pictures.",
          "cameraPermission": "Allow Intentional Dating to access your camera to take profile photos."
        }
      ]
    ],
    "scheme": "intentionaldating",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "backendUrl": "https://6ytjugugmhrw5w5dguny2u79uz83r3tz.app.specular.dev",
      "eas": {
        "projectId": "YOUR_PROJECT_ID_HERE"
      }
    },
    "owner": "YOUR_EXPO_USERNAME",
    "updates": {
      "url": "https://u.expo.dev/YOUR_PROJECT_ID_HERE"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}