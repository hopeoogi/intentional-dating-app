{
  "expo": {
    "name": "Intentional",
    "slug": "intentional-dating",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/natively-dark.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/natively-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6B9D"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.intentional.dating",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "We need access to your camera to take profile photos and videos.",
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to upload profile photos and videos.",
        "NSLocationWhenInUseUsageDescription": "We use your location to show you matches nearby."
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:intentional.dating"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/natively-dark.png",
        "backgroundColor": "#FF6B9D"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.intentional.dating",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "intentional.dating",
              "pathPrefix": "/auth"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Intentional to access your photos to upload profile pictures.",
          "cameraPermission": "Allow Intentional to access your camera to take profile pictures."
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow Intentional to use your location to find matches nearby."
        }
      ],
      [
        "expo-apple-authentication",
        {
          "appleTeamId": "YOUR_APPLE_TEAM_ID"
        }
      ]
    ],
    "scheme": "intentional",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "backendUrl": "https://6ytjugugmhrw5w5dguny2u79uz83r3tz.app.specular.dev",
      "eas": {
        "projectId": "your-project-id-here"
      }
    }
  }
}